{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
"config": {
        "includeFiles": [
          "web/build/index.html",
          "web/build/favicon.ico",
          "web/build/assets/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}